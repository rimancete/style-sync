{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:commit": "eslint --config eslint.commit.mjs \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:commit:check": "eslint --config eslint.commit.mjs \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:unit": "jest --testPathPattern=\".*\\.\\(test|spec\\)\\.ts$\" --testPathIgnorePatterns=\".*\\.integration\\.(test|spec)\\.ts$\" --testPathIgnorePatterns=\".*\\.e2e\\.(test|spec)\\.ts$\" --testPathIgnorePatterns=\".*\\.contract\\.(test|spec)\\.ts$\"",
    "test:integration": "jest --testPathPattern=\".*\\.integration\\.(test|spec)\\.ts$\"",
    "test:watch": "jest --watch",
    "test:watch:unit": "jest --watch --testPathPattern=\".*\\.\\(test|spec\\)\\.ts$\" --testPathIgnorePatterns=\".*\\.integration\\.(test|spec)\\.ts$\" --testPathIgnorePatterns=\".*\\.e2e\\.(test|spec)\\.ts$\" --testPathIgnorePatterns=\".*\\.contract\\.(test|spec)\\.ts$\"",
    "test:watch:integration": "jest --watch --testPathPattern=\".*\\.integration\\.(test|spec)\\.ts$\"",
    "test:cov": "jest --coverage",
    "test:cov:unit": "jest --coverage --testPathPattern=\".*\\.\\(test|spec\\)\\.ts$\" --testPathIgnorePatterns=\".*\\.integration\\.(test|spec)\\.ts$\" --testPathIgnorePatterns=\".*\\.e2e\\.(test|spec)\\.ts$\" --testPathIgnorePatterns=\".*\\.contract\\.(test|spec)\\.ts$\"",
    "test:cov:integration": "jest --coverage --testPathPattern=\".*\\.integration\\.(test|spec)\\.ts$\"",
    "validate:commit": "commitlint --from HEAD~1 --to HEAD --verbose",
    "test:contracts": "jest --testPathPattern=\".*\\.contract\\.(test|spec)\\.ts$\"",
    "test:watch:contracts": "jest --watch --testPathPattern=\".*\\.contract\\.(test|spec)\\.ts$\"",
    "test:cov:contracts": "jest --coverage --testPathPattern=\".*\\.contract\\.(test|spec)\\.ts$\"",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "ts-node prisma/seed.ts",
    "env:validate": "node scripts/validate-env.js",
    "env:setup-git-crypt": "./scripts/setup-git-crypt.sh",
    "start:staging": "NODE_ENV=staging npm start",
    "start:production": "NODE_ENV=production npm start",
    "prisma:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "prepare": "husky",
    "commit": "cz",
    "commit:retry": "cz --retry"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@prisma/client": "^6.14.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.14.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-json-schema": "^6.1.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.(spec|test)\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --config eslint.commit.mjs --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog",
      "maxHeaderWidth": 72,
      "maxLineWidth": 100,
      "defaultType": "feat",
      "defaultScope": "core"
    }
  }
}
